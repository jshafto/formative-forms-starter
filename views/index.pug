extends layout

block content
    h2 Existing Users
    table
        thead
            tr
                th id
                th First Name
                th Last Name
                th Email
                th Age
                th Favorite Beatle
                th Likes Ice Cream
        tbody
            each item in users
                tr
                    td= item.id
                    td= item.firstName
                    td= item.lastName
                    td= item.email
                    td= item.age
                    td= item.favoriteBeatle
                    td= item.LikesIceCream
